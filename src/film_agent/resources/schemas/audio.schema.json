      "title": "Image Prompt Package Id",
      "type": "string"
    },
    "music_prompt": {
      "title": "Music Prompt",
      "type": "string"
    },
    "selected_images_id": {
      "title": "Selected Images Id",
      "type": "string"
    },
    "shot_prompts": {
      "items": {
        "$ref": "#/$defs/AVPromptItem"
